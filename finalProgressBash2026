//////////////////////////////    from Gemini 3 flash   //////////////



2. The Unified Prisma Schema (MongoDB)
This schema powers the entire "OS" + "Marketplace" + "Bank". It merges your requested structures into a clean, 
single-source-of-truth.

File: packages/database/prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// ==========================================
// 1. IDENTITY & ROLES (The "OS" Layer)
// ==========================================

model User {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  email       String    @unique
  name        String
  phone       String?
  role        UserRole  @default(SUB_CONTRACTOR)
  companyId   String?   @db.ObjectId
  company     Company?  @relation(fields: [companyId], references: [id])
  createdAt   DateTime  @default(now())
  
  // Relations
  projectsOwned    Project[] @relation("ProjectOwner")
  bidsSubmitted    BidSubmission[]
  tasksAssigned    Task[]
  notifications    Notification[]
}

model Company {
  id            String       @id @default(auto()) @map("_id") @db.ObjectId
  name          String
  type          CompanyType
  ein           String?      @unique
  bondingLimit  Float?       // Max project size they can take
  insuranceDocs InsuranceDocument[]
  users         User[]
  createdAt     DateTime     @default(now())
}

enum UserRole {
  GC_ADMIN
  SUPERINTENDENT
  SUB_CONTRACTOR
  WORKER
}

enum CompanyType {
  GENERAL_CONTRACTOR
  SUBCONTRACTOR
  INSURANCE_CARRIER
}

// ==========================================
// 2. PROJECT INTELLIGENCE (The "Brain" Layer)
// ==========================================

model Project {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  name           String
  address        String
  ownerId        String    @db.ObjectId
  owner          User      @relation("ProjectOwner", fields: [ownerId], references: [id])
  status         ProjectStatus @default(PLANNING)
  
  // The "Brain" Components
  blueprints     Blueprint[]
  rfis           RFI[]
  safetyLogs     SafetyLog[]
  tasks          Task[]
  bids           Bid[]
  payments       Payment[]
  
  createdAt      DateTime  @default(now())
}

model Blueprint {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  projectId   String   @db.ObjectId
  project     Project  @relation(fields: [projectId], references: [id])
  fileUrl     String
  version     Int      @default(1)
  
  // AI Analysis Results
  vectorId    String?  // Reference to Qdrant embedding
  summary     String?
  components  Json?    // { "electrical": ["switch_2a", "panel_b"], "hvac": [...] }
  clashes     Json?    // [{ "x": 10, "y": 20, "issue": "Pipe hits Beam" }]
  
  processedAt DateTime?
}

model RFI {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  projectId      String   @db.ObjectId
  project        Project  @relation(fields: [projectId], references: [id])
  
  question       String
  suggestedAnswer String? // AI Auto-Draft
  status         RFIStatus @default(OPEN)
  
  impactCost     Float?
  impactDays     Int?
  
  createdAt      DateTime @default(now())
}

// ==========================================
// 3. MARKETPLACE & BIDDING (The "Wedge")
// ==========================================

model Bid {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  projectId   String   @db.ObjectId
  project     Project  @relation(fields: [projectId], references: [id])
  
  title       String
  budgetMax   Float?
  trade       String   // e.g. "Plumbing"
  deadline    DateTime
  
  submissions BidSubmission[]
  status      BidStatus @default(OPEN)
}

model BidSubmission {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  bidId       String   @db.ObjectId
  bid         Bid      @relation(fields: [bidId], references: [id])
  userId      String   @db.ObjectId
  user        User     @relation(fields: [userId], references: [id])
  
  amount      Float
  proposalUrl String
  aiScore     Float?   // 0-100 Match Score based on history/bonding
  
  status      SubmissionStatus @default(PENDING)
}

// ==========================================
// 4. FINTECH & COMPLIANCE (The "Bank")
// ==========================================

model Payment {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  projectId   String    @db.ObjectId
  project     Project   @relation(fields: [projectId], references: [id])
  
  amount      Float
  payeeId     String    @db.ObjectId // Subcontractor Company ID
  
  // "PayBrain" Logic
  taskId      String?   @db.ObjectId
  task        Task?     @relation(fields: [taskId], references: [id])
  
  verificationPhoto String?
  gpsCoords         String?
  aiVerified        Boolean @default(false)
  
  status      PaymentStatus @default(PENDING_VERIFICATION)
  stripeId    String?
  
  // Factoring
  isFactored  Boolean   @default(false)
  factorFee   Float     @default(0.0) // If instant pay, this is populated
}

model InsuranceDocument {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  companyId    String   @db.ObjectId
  company      Company  @relation(fields: [companyId], references: [id])
  
  type         String   // "GL", "WorkersComp"
  expiryDate   DateTime
  coverage     Float
  fileUrl      String
  
  isValid      Boolean  @default(false)
  verificationLog String? // "OCR detected match with requirements"
}

// ==========================================
// 5. FIELD OPERATIONS
// ==========================================

model Task {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  projectId   String   @db.ObjectId
  project     Project  @relation(fields: [projectId], references: [id])
  assignedTo  String?  @db.ObjectId
  user        User?    @relation(fields: [assignedTo], references: [id])
  
  title       String
  status      TaskStatus @default(PENDING)
  
  // Geofencing
  geoLat      Float?
  geoLng      Float?
  
  payment     Payment[]
}

model SafetyLog {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  projectId   String   @db.ObjectId
  project     Project  @relation(fields: [projectId], references: [id])
  
  imageUrl    String
  violations  Json?    // ["No Hard Hat", "Unsafe Ladder"]
  riskScore   Float
  resolved    Boolean  @default(false)
  
  createdAt   DateTime @default(now())
}

model Notification {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  userId    String   @db.ObjectId
  user      User     @relation(fields: [userId], references: [id])
  message   String
  read      Boolean  @default(false)
  type      String   // "BID_ALERT", "SAFETY_WARN", "PAYMENT_SENT"
}

// Enums
enum ProjectStatus { PLANNING; ACTIVE; HOLD; COMPLETED }
enum RFIStatus { OPEN; DRAFTED; SENT; RESOLVED }
enum BidStatus { OPEN; CLOSED; AWARDED }
enum SubmissionStatus { PENDING; REJECTED; ACCEPTED }
enum PaymentStatus { PENDING_VERIFICATION; VERIFIED; PAID; FAILED }
enum TaskStatus { PENDING; IN_PROGRESS; DONE_PENDING_REVIEW; COMPLETED }



///////////////





