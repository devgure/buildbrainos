generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
  id        String @id @map("_id") @default(auto()) @db.ObjectId
  name      String
  address   String?
  ownerId   String
  createdAt DateTime @default(now())
}
