generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Bid {
  id        String @id @map("_id") @default(auto()) @db.ObjectId
  title     String
  budget    Int?
  source    String?
  createdAt DateTime @default(now())
}
